{
    "name": "wolnosciowiec/file-repository",
    "type": "project",
    "description": "File storage service, built for ultra-low budget shared hostings with no footprint on performance",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "blocktrail/cryptojs-aes-php": "^0.1.0",
        "gabrielelana/byte-units": "^0.5.0",
        "glenscott/url-normalizer": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "kendrick/symfony-debug-toolbar-git": "^1.4",
        "league/flysystem-aws-s3-v2": "^1.0",
        "oneup/flysystem-bundle": "^3.0",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/console": "*",
        "symfony/debug-pack": "^1.0",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/orm-pack": "^1.0",
        "symfony/security": "*",
        "symfony/security-bundle": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*",
        "twistor/flysystem-stream-wrapper": "^1.0"
    },
    "require-dev": {
        "symfony/dotenv": "*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "update-scripts": [
            "rm -rf ./var/cache/* || true",
            "./bin/console cache:clear --env=prod -vvv",
            "./bin/console cache:clear --env=test || true",
            "./bin/console cache:clear --env=dev || true",
            "./bin/console assets:install"
        ],
        "post-install-cmd": [
            "@update-scripts"
        ],
        "post-update-cmd": [
            "@update-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
